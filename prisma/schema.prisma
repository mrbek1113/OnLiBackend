generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  surname  String
  username String @unique
  password String
  role     Role   @default(user)
}
model Product{
  id      Int    @id @default(autoincrement())
  name    String ?
  img     String ?
  file    String ?
  desc    String ?

}

enum Role {
  user
  admin
}
